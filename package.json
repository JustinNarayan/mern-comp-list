{
  "name": "mern-comp-list",
  "version": "1.0.0",
  "description": "Basic MERN stack application for practice",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
